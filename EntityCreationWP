CREATE (q:post { name: 'Lightpole17', location: 'Bayswater', bulbtype: 'LED'})
CREATE (r1:post { name: 'Lightpole18', location: 'Bayswater', bulbtype: 'LED'})

======================= CREATE GENERATORS ============================================

CREATE (z6:generator { name: 'CollieGen', location: 'Collie'})
CREATE (z7:generator { name: 'BlueWaterGen', location: 'Collie'})
CREATE (z8:generator { name: 'MujaGen', location: 'Collie'})

====================== CREATE TRANSFORMERS ============================================

CREATE (z1:transformer { name: 'BaysTransformer'})
CREATE (z2:transformer { name: 'MayTransformer'})

===================== CREATE POWERLINE ============================================

CREATE (z3:pwrline { name: 'BaysLine'})
CREATE (z4:pwrline { name: 'MayLine'})

===================== CREATE INSPECTION LOGS =======================================

CREATE (s:saftey { name: 'InspectionLog1'})
CREATE (t:saftey { name: 'InspectionLog2'})

====================== CREATE WORKERS =====================================================

//TECHNICIANS + MANAGERS
CREATE (v:employee { name: 'John', role: 'Technician', workType: 'Full Time'})
CREATE (w:employee { name: 'Carol', role: 'Technician', workType: 'Full Time'})
CREATE (x:employee { name: 'Max', role: 'Supervisor', workType: 'Full Time'})
CREATE (y:employee { name: 'Jean', role: 'Manager', workType: 'Full Time'})
CREATE (z:employee { name: 'Ben', role: 'Technician', workType: 'Contract'})
CREATE (z5:employee { name: 'Jess', role: 'Technician', workType: 'Part Time'})

//POWERPLANT WORKERS + MANAGERS
CREATE (v1:employee { name: 'Jack', role: 'Technician', workType: 'Full Time', location: 'CollieGen'})
CREATE (v2:employee { name: 'Rob', role: 'Technician', workType: 'Full Time', location: 'CollieGen'})
CREATE (v3:employee { name: 'Stacy', role: 'Technician', workType: 'Full Time', location: 'CollieGen'})
CREATE (v4:employee { name: 'Jen', role: 'Technician', workType: 'Full Time', location: 'BlueWaterGen'})
CREATE (v5:employee { name: 'Matt', role: 'Technician', workType: 'Full Time', location: 'BlueWaterGen'})
CREATE (v6:employee { name: 'Drew', role: 'Technician', workType: 'Full Time', location: 'BlueWaterGen'})
CREATE (v7:employee { name: 'Lisa', role: 'Technician', workType: 'Full Time', location: 'MujaGen'})
CREATE (v8:employee { name: 'Alex', role: 'Technician', workType: 'Full Time', location: 'MujaGen'})
CREATE (v9:employee { name: 'Ethan', role: 'Technician', workType: 'Full Time', location: 'MujaGen'})
CREATE (v10:employee { name: 'Jordan', role: 'Supervisor', workType: 'Full Time', location: 'HQ'})
CREATE (v11:employee { name: 'Sam', role: 'Manager', workType: 'Full Time', location: 'HQ'})
CREATE (v12:employee { name: 'Josh', role: 'Area Manager', workType: 'Full Time', location: 'HQ'})


======================= CREATE RELATIONSHIPS =================================
--------------------- (ENTER SEPERATLY)----------------------------------
------------ MAYLANDS LIGHTS AND MAYLANDS TRANSFORMER-----------------------

MATCH (a:post),(z2:transformer)
WHERE a.name = 'Lightpole1' AND z2.name = 'MayTransformer'
CREATE (a)-[r:CONNECTED_TO]-> (z2)

MATCH (b:post),(z2:transformer)
WHERE b.name = 'Lightpole2' AND z2.name = 'MayTransformer'
CREATE (b)-[r:CONNECTED_TO]-> (z2)

MATCH (c:post),(z2:transformer)
WHERE c.name = 'Lightpole3' AND z2.name = 'MayTransformer'
CREATE (c)-[r:CONNECTED_TO]-> (z2)

MATCH (d:post),(z2:transformer)
WHERE d.name = 'Lightpole4' AND z2.name = 'MayTransformer'
CREATE (d)-[r:CONNECTED_TO]-> (z2)

MATCH (e:post),(z2:transformer)
WHERE e.name = 'Lightpole5' AND z2.name = 'MayTransformer'
CREATE (e)-[r:CONNECTED_TO]-> (z2)

MATCH (f:post),(z2:transformer)
WHERE f.name = 'Lightpole6' AND z2.name = 'MayTransformer'
CREATE (f)-[r:CONNECTED_TO]-> (z2)

MATCH (g:post),(z2:transformer)
WHERE g.name = 'Lightpole7' AND z2.name = 'MayTransformer'
CREATE (g)-[r:CONNECTED_TO]-> (z2)

MATCH (h:post),(z2:transformer)
WHERE h.name = 'Lightpole8' AND z2.name = 'MayTransformer'
CREATE (h)-[r:CONNECTED_TO]-> (z2)


