// BLUEWATER GEN

MATCH (v4:employee),(z7:generator)
WHERE v4.name = 'Jen' AND z7.name = 'BlueWaterGen'
CREATE (v4)-[r:WORKS_AT]-> (z7)

MATCH (v5:employee),(z7:generator)
WHERE v5.name = 'Matt' AND z7.name = 'BlueWaterGen'
CREATE (v5)-[r:WORKS_AT]-> (z7)

MATCH (v6:employee),(z7:generator)
WHERE v6.name = 'Drew' AND z7.name = 'BlueWaterGen'
CREATE (v6)-[r:WORKS_AT]-> (z7)

----------------------------- CREATE MANAGER SUPER REL -----------------------------------------

MATCH (x:employee),(y:employee)
WHERE x.name = 'Max' AND y.name = 'Jean'
CREATE (y)-[r:MANAGES]-> (x)

MATCH (v11:employee),(v10:employee)
WHERE v11.name = 'Sam' AND v10.name = 'Jordan'
CREATE (v11)-[r:MANAGES]-> (v10)

------------------------------- CREATE AREA MANAGER REL ------------------------------------------

MATCH (v12:employee),(v11:employee)
WHERE v12.name = 'Josh' AND v11.name = 'Sam'
CREATE (v12)-[r:MANAGES]-> (v11)

MATCH (v12:employee),(y:employee)
WHERE v12.name = 'Josh' AND y.name = 'Jean'
CREATE (v12)-[r:MANAGES]-> (y)

------------------------------- QUERIES ---------------------------------------------------------

//SHOW POSTS
match (n:post) return n

//SHOW POSTS WITH ONLY LEDS OR HALOGEN
match (n:post) where n.bulbtype = "<LED/HALOGEN>" return n

//SHOW POSTS THAT RESIDE IN BAYSWATER/MAYLANDS
match (n:post) where n.location = "<Bayswater/Maylands>" return n

//SHOW WORKER BY ROLE AND FULL TIME WORKTYPE
match (n:employee) where n.role = "Technician" and n.workType = "Full Time" return n

//LIST WORKERS
match (n:employee) return n {.name, .workType, .role}

//LIST WORKERS THAT WORK AT COLLIE GEN
match (n:employee)-[WORKS_AT]-(generator) where generator.name = "CollieGen" return n, generator

//SHOW ALL POSTS CONNECTED TO A TRANSFOMRER
match (n:post)-[CONNECTED_TO]-(transformer) where transformer.name = "MayTransformer" return n, transformer

//SHOW ALL POSTS CONNECTED TO TRANSFORMER THAT HAVE LED BULBTYPE
match (n:post)-[CONNECTED_TO]-(transformer) where transformer.name = "MayTransformer" and n.bulbtype = "LED" return n, transformer
