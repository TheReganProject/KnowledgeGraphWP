WHERE r1.name = 'Lightpole18' AND z1.name = 'BaysTransformer'
CREATE (r1)-[r:CONNECTED_TO]-> (z1)

---------------------------- CREATE TRANSFORMER PWRLINE REL ---------------------------------------

MATCH (z3:pwrline),(z1:transformer)
WHERE z3.name = 'BaysLine' AND z1.name = 'BaysTransformer'
CREATE (z3)-[r:CARRIES_POWER_TO]-> (z1)

MATCH (z4:pwrline),(z2:transformer)
WHERE z4.name = 'MayLine' AND z2.name = 'MayTransformer'
CREATE (z4)-[r:CARRIES_POWER_TO]-> (z2)


----------------------------- CREATE PWRLINE GENERATOR REL ---------------------------------------

MATCH (z4:pwrline),(z6:generator)
WHERE z4.name = 'MayLine' AND z6.name = 'CollieGen'
CREATE (z6)-[r:SENDS_POWER_TO]-> (z4)

MATCH (z3:pwrline),(z7:generator)
WHERE z3.name = 'BaysLine' AND z7.name = 'BlueWaterGen'
CREATE (z7)-[r:SENDS_POWER_TO]-> (z3)

MATCH (z4:pwrline),(z8:generator)
WHERE z4.name = 'MayLine' AND z8.name = 'MujaGen'
CREATE (z8)-[r:SENDS_POWER_TO]-> (z4)

MATCH (z3:pwrline),(z8:generator)
WHERE z3.name = 'BaysLine' AND z8.name = 'MujaGen'
CREATE (z8)-[r:SENDS_POWER_TO]-> (z3)

--------------------------- CREATE INSPECTION LOG AND TRANSFORMER REL -----------------------------------

MATCH (z2:transformer),(s:saftey)
WHERE z2.name = 'MayTransformer' AND s.name = 'InspectionLog1'
CREATE (z2)-[r:HAS]-> (s)

MATCH (z1:transformer),(t:saftey)
WHERE z1.name = 'BaysTransformer' AND t.name = 'InspectionLog2'
CREATE (z1)-[r:HAS]-> (t)

--------------------------- CREATE INSPECTION LOG AND WORKER REL -----------------------------------------

//BAYSWATER
MATCH (v:employee),(t:saftey)
WHERE v.name = 'John' AND t.name = 'InspectionLog2'
CREATE (v)-[r:WORKS_ON]-> (t)

MATCH (w:employee),(t:saftey)
WHERE w.name = 'Carol' AND t.name = 'InspectionLog2'
CREATE (w)-[r:WORKS_ON]-> (t)

//MAYLANDS

MATCH (z:employee),(s:saftey)
WHERE z.name = 'Ben' AND s.name = 'InspectionLog1'
CREATE (z)-[r:WORKS_ON]-> (s)

MATCH (z5:employee),(s:saftey)
WHERE z5.name = 'Jess' AND s.name = 'InspectionLog1'
CREATE (z5)-[r:WORKS_ON]-> (s)

--------------------------- CREATE SUPERVISOR WORKER REL ------------------------------------------
//INSPECTION WORKERS
MATCH (x:employee),(v:employee)
WHERE x.name = 'Max' AND v.name = 'John'
CREATE (x)-[r:SUPERVISES]-> (v)

MATCH (x:employee),(w:employee)
WHERE x.name = 'Max' AND w.name = 'Carol'
CREATE (x)-[r:SUPERVISES]-> (w)

MATCH (x:employee),(z:employee)
WHERE x.name = 'Max' AND z.name = 'Ben'
CREATE (x)-[r:SUPERVISES]-> (z)

MATCH (x:employee),(z5:employee)
WHERE x.name = 'Max' AND z5.name = 'Jess'
CREATE (x)-[r:SUPERVISES]-> (z5)

//GEN WORKERS

MATCH (v10:employee),(v1:employee)
WHERE v10.name = 'Jordan' AND v1.name = 'Jack'
CREATE (v10)-[r:SUPERVISES]-> (v1)

MATCH (v10:employee),(v2:employee)
WHERE v10.name = 'Jordan' AND v2.name = 'Rob'
CREATE (v10)-[r:SUPERVISES]-> (v2)

MATCH (v10:employee),(v3:employee)
WHERE v10.name = 'Jordan' AND v3.name = 'Stacy'
CREATE (v10)-[r:SUPERVISES]-> (v3)

MATCH (v10:employee),(v4:employee)
WHERE v10.name = 'Jordan' AND v4.name = 'Jen'
CREATE (v10)-[r:SUPERVISES]-> (v4)

MATCH (v10:employee),(v5:employee)
WHERE v10.name = 'Jordan' AND v5.name = 'Matt'
CREATE (v10)-[r:SUPERVISES]-> (v5)

MATCH (v10:employee),(v6:employee)
WHERE v10.name = 'Jordan' AND v6.name = 'Drew'
CREATE (v10)-[r:SUPERVISES]-> (v6)

MATCH (v10:employee),(v7:employee)
WHERE v10.name = 'Jordan' AND v7.name = 'Lisa'
CREATE (v10)-[r:SUPERVISES]-> (v7)

MATCH (v10:employee),(v8:employee)
WHERE v10.name = 'Jordan' AND v8.name = 'Alex'
CREATE (v10)-[r:SUPERVISES]-> (v8)

MATCH (v10:employee),(v9:employee)
WHERE v10.name = 'Jordan' AND v9.name = 'Ethan'
CREATE (v10)-[r:SUPERVISES]-> (v9)


----------------------------- CREATE WORKER GEN REL ----------------------------------------------
//COLLIE GEN
MATCH (v1:employee),(z6:generator)
WHERE v1.name = 'Jack' AND z6.name = 'CollieGen'
CREATE (v1)-[r:WORKS_AT]-> (z6)

MATCH (v2:employee),(z6:generator)
WHERE v2.name = 'Rob' AND z6.name = 'CollieGen'
CREATE (v2)-[r:WORKS_AT]-> (z6)

MATCH (v3:employee),(z6:generator)
WHERE v3.name = 'Stacy' AND z6.name = 'CollieGen'
CREATE (v3)-[r:WORKS_AT]-> (z6)

//MUJA GEN

MATCH (v7:employee),(z8:generator)
